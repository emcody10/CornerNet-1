{
    "system": {

        "debug": false,
        "dataset": "MSCOCO",
        "batch_size": 40,
        "sampling_function": "kp_detection",

        "train_split": "trainval",
        "val_split": "minival",

        "learning_rate": 0.00025,
        "decay_rate": 1,

        "val_iter": 100,

        "opt_algo": "sgd",
        "prefetch_size": 5,

        "max_iter": 500000,
        "stepsize": 450000,
        "snapshot": 1000,

        "chunk_sizes": [5, 5, 5, 5, 5, 5, 5, 5],

        "data_dir": "./data"
    },
    
    "db": {
        "data_aug": false,
        "lighting": false,
        "rand_scale_min": 1.0,
        "rand_scale_max": 1.0,
        "rand_scale_step": 0.1,
        "rand_scales": null,

        "rand_crop": false,
        "rand_color": false,

        "border": 128,
        "gaussian_bump": true,
        "gaussian_iou": 0.3,

        "input_size": [512, 512],
        "output_sizes": [[128, 128]],

        "test_scales": [1],

        "top_k": 100,
        "categories": 6,
        "ae_threshold": 0.5,
        "nms_threshold": 0.3,

        "max_per_image": 100
    }
}
